{
  "expo": {
    "name": "centerdaniil",
    "slug": "centerdaniil",
    "privacy": "public",
    "version": "1.2.0",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#21BCD4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.vasiliyzaykov.centerdaniil",
      "buildNumber": "1.2.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "Your calendar is used to save events",
        "NSCameraUsageDescription": "Your camera is used to scan qr-codes and move to events"
      }
    },
    "android": {
      "package": "com.vasiliyzaykov.centerdaniil",
      "versionCode": 1,
      "permissions": ["READ_CALENDAR"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"       
      },
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
